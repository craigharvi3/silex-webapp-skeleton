{
    "name": "craigharvi3/silex-webapp-skeleton",
    "description": "A simple, scalable and pretty beautiful silex skeleton for writing web applications",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Craig Harvie",
            "email": "harvie5@msn.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/yaml": "^2.1|^3.0"
    },
    "require-dev": {
        "silex/silex": "^2.0",
        "silex/web-profiler": "^2.0",
        "twig/twig": "~1.8",
        "symfony/twig-bridge": "^3.0",
        "symfony/monolog-bridge": "^3.0",
        "symfony/console": "^3.1",
        "doctrine/common": "~2.0",
        "monolog/monolog": "~1.0",
        "solution10/config": "~2.0",
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^2.0",
        "symfony/browser-kit": "^3.0",
        "symfony/css-selector": "^3.0",
        "johnkary/phpunit-speedtrap": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CH\\Skeleton\\": "core/src/",
            "CH\\Home\\": "app/Home/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CH\\Skeleton\\Tests\\": "core/tests/",
            "CH\\Home\\Tests\\": "apps/Home/tests"
        }
    }
}
